<div class="wrapper">
    <div class="title-text">
        <div class="title login">
            Passwords
        </div>
    </div>    
    
    <div style="padding-top:10px">
         <a style="" class="btn btn-primary btn-md" href="/add_new">Add New</a>
    </div>
   

    <div class="form-inner">
        <ul id="list">
            {{#each passwords as |password| }}
                <li><a href="/passwords/{{password.id}}">
                        <div>{{password.website}}</div>
                        <div style="color:gray">Last updated: {{format_date password.updatedAt}}</div>
                    </a>
                </li>
            {{/each}}
        </ul>
    </div>
    
</div>
        
